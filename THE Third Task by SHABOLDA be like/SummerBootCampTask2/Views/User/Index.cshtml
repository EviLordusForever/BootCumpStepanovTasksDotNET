@model IEnumerable<SummerBootCampTask2.CoreModels.User>

<img width="400" height="200" tabindex="0" class=" nofocus" aria-label="Смотреть исходное изображение" alt="Смотреть исходное изображение" src="http://tse1.mm.bing.net/th/id/OIP.mfWkB1ahfdMQagBfUF7VRgAAAA?pid=ImgDet&amp;rs=1">

<h1>Users</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
            </th>
        </tr>
    </thead>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                <a asp-controller="Chat" asp-action="Index" asp-route-id="@item.Id">Chat</a>
            </td>
        </tr>
    }
</table>